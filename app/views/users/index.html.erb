<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
      <li>
        Name: <%= user.name %>
        <span>
          <%= friend_btn(user) %>
          <%= reject_btn(user) %>
        </span>
        <span class="profile-link">
          <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
        </span><br><br>
        <%= mutual_friends(user)%>
      </li><br><br>
    <% end %>
  </ul>
</section>
